  margin-right: 7rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.model-1-form-select:last-child {
  float: right;
  margin-right: 0;
}

.model-1-form-arrow {
  position: absolute;
  width: 24rpx;
