/* 这里的样式只应用于这个自定义组件 */

.exchange {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 1;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.6);
  font-family: 'MFJiaHei';
  overflow: hidden;
  box-sizing: border-box;
}

.exchange-content {
  width: 649rpx;
  height: 1003rpx;
  margin: 0 auto;
  position: relative;
  overflow: hidden;
  margin-top: 106rpx;
}

.exchange-bg {
  width: 649rpx;
  height: 1003rpx;
  margin: 0 auto;
  position: absolute;
  z-index: 0;
  left: 0;
  top: 0;
}

.exchange-close {
  width: 57rpx;
  height: 66rpx;
  position: absolute;
  z-index: 1;
  right: 25rpx;
  top: 23rpx;
}

.exchange-model-1 {
  position: relative;
}

.model-1-title {
  color: #182b53;
  font-size: 56rpx;
  text-align: center;
  padding-top: 75rpx;
  padding-bottom: 68rpx;
  line-height: 1;
}

